{
  "name": "website",
  "version": "0.0.1",
  "description": "Drash Land",
  "type": "module",
  "scripts": {
    "compile:drash-v1.x-api-reference": "console/compile_drash_v1.x_api_reference",
    "compile:vue-routes": "node console/compile_vue_routes.js",
    "docker:build-production": "docker build --no-cache --build-arg APP_ENV=production -t drashland-website:latest .",
    "docker:build-staging": "docker build --no-cache --build-arg APP_ENV=staging -t drashland-website:staging-latest .",
    "docker:production": "yarn docker:build-production && yarn docker:tag-production && yarn docker:push-production",
    "docker:push-production": "docker push registry.digitalocean.com/drashland/drashland-website:latest",
    "docker:push-staging": "docker push registry.digitalocean.com/drashland/drashland-website:staging-latest",
    "docker:staging": "yarn docker:build-staging && yarn docker:tag-staging && yarn docker:push-staging",
    "docker:tag-production": "docker tag drashland-website:latest registry.digitalocean.com/drashland/drashland-website:latest",
    "docker:tag-staging": "docker tag drashland-website:staging-latest registry.digitalocean.com/drashland/drashland-website:staging-latest",
    "remove-ds-store": "find . -name '.DS_Store' -type f -delete",
    "server": "deno run -A console/server.ts",
    "webpack:development": "webpack --config ./webpack.config.js --env development",
    "webpack:production": "webpack --config ./webpack.config.js --env production",
    "webpack:staging": "webpack --config ./webpack.config.js --env staging"
  },
  "author": "Drash Land",
  "devDependencies": {
    "@babel/core": "7.x",
    "axios": "0.x",
    "babel-loader": "8.x",
    "clipboard": "2.x",
    "css-loader": "3.x",
    "markdown-it": "11.x",
    "node-sass": "4.x",
    "prismjs": "1.x",
    "pug": "3.x",
    "pug-plain-loader": "1.x",
    "sass-loader": "9.x",
    "scss-loader": "0.x",
    "style-loader": "1.x",
    "terser-webpack-plugin": "^5.1.4",
    "vue": "2.x",
    "vue-loader": "15.x",
    "vue-router": "3.x",
    "vue-style-loader": "4.x",
    "vue-template-compiler": "2.x",
    "webpack": "5.x",
    "webpack-cli": "4.x"
  }
}
